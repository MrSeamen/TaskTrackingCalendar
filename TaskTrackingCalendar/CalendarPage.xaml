<Window x:Class="TaskTrackingCalendar.CalendarPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskTrackingCalendar"
        mc:Ignorable="d"
        Title="CalendarPage" Height="450" Width="800"
        WindowStartupLocation="CenterScreen"
        Closing="OnClose">
    <Grid>
        <Button Margin="5" Padding="5" Click="OnOpenSummary" Height="50" Width="100">
            <TextBlock Text="Open Summary Page" TextWrapping="Wrap" TextAlignment="Center"/>
        </Button>
    </Grid>
</Window>
